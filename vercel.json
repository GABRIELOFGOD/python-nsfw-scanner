{
    "builds": [
        {
            "src": "online/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdasize": "15mb", "runtime": "python3.10" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "online/wsgi.py"
        }
    ]
}